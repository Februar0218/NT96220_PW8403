	-D_FWCOMPRESS_NONE_=0 \
	-D_FWCOMPRESS_FULL_=1 \
	-D_FWCOMPRESS_PARTIAL_=2 \
	-D_FWCOMPRESS_NONE_PARTIAL_=3 \
