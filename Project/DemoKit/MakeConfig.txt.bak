    DRAM_SIZE_VALUE = 0x00800000
endif

ifeq "$(DRAMSIZE)" "SIZE_16MB"
    DRAM_SIZE_VALUE = 0x01000000
endif

ifeq "$(DRAMSIZE)" "SIZE_32MB"
    DRAM_SIZE_VALUE = 0x02000000
endif

