	-PD "_FWCOMPRESS_NONE_ SETA 0" \
	-PD "_FWCOMPRESS_FULL_ SETA 1" \
	-PD "_FWCOMPRESS_PARTIAL_ SETA 2" \
	-PD "_FWCOMPRESS_NONE_PARTIAL_ SETA 3" \
